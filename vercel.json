{
    "version": 2,
    "installCommand": "./scripts/install.sh -o linux -p x64",
    "buildCommand": "./scripts/build.sh",
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/index.go"
        },
        {
            "source": "/markdown",
            "destination": "/api/markdown.go"
        }
    ],
    "functions": {
        "api/*.go": {
            "includeFiles": "assets/**/*"
        }
    }
}
